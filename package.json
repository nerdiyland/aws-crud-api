{
  "name": "@nerdiyland/aws-crud-api-rest",
  "version": "2.159.1",
  "description": "A powerful AWS CDK construct that enables you to create fully-featured CRUD APIs with minimal configuration",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "packages/standard-crud-backend/**/*",
    "!packages/standard-crud-backend/node_modules",
    "!packages/standard-crud-backend/package-lock.json",
    "static/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf lib && rm -rf packages/standard-crud-backend/lib",
    "prepublishOnly": "npm run clean && npm run lint:check && npm run format:check && npm run build && npm run test",
    "validate": "npm run lint:check && npm run format:check && npm run build && npm run test",
    "validate:infrastructure": "npm run lint:check && npm run format:check && npm run build && npm run test:infrastructure",
    "prepublish": "lerna clean --yes",
    "postinstall": "npm run bootstrap",
    "bootstrap": "lerna exec -- npm i --omit=dev"
  },
  "keywords": [
    "aws",
    "cdk",
    "crud",
    "api",
    "rest",
    "api-gateway",
    "lambda",
    "dynamodb",
    "serverless",
    "construct",
    "typescript"
  ],
  "author": "Pelayo Sanchez <nerdiyland@gmail.com>",
  "license": "MIT",
  "homepage": "https://nerdiyland.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/nerdiyland/aws-crud-api.git"
  },
  "bugs": {
    "url": "https://github.com/nerdiyland/aws-crud-api/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.68.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.0",
    "husky": "^6.0.0",
    "jest": "^29.0.0",
    "lerna": "^8.2.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5.5.4",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.25.1"
  }
}
